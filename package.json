{
  "name": "studio-public",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'app/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check 'app/**/*.{js,jsx,ts,tsx,css,md,json,html}'",
    "format": "prettier --write 'app/**/*.{js,jsx,ts,tsx,css,md,json,html}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,md,json,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "autoprefixer": "10.4.16",
    "next": "13.5.2",
    "next-themes": "^0.2.1",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-datepicker": "^4.18.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.1"
  }
}
